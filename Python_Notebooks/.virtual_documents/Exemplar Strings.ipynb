def url_checker(url):
    protocol=url.split('/')[0]
    store_ID=url.split('/')[-1]
    if protocol!='https:' and len(store_ID)!=7:
        print(f"{protocol} is an invalid protocol")
        print(f"{store_ID} is an invalid storeID")
    elif protocol!='https:' and len(store_ID)==7:
        print(f"{protocol} is an invalid protocol")
    elif protocol=='https:' and len(store_ID)!=7:
        print(f"{store_ID} is an invalid storeID")
    else:
        return store_ID





